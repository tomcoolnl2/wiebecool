{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/next"
        },
        {
            "src": "storybook-static/**/*",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/storybook/(.*)",
            "dest": "/storybook-static/$1"
        }
    ]
}